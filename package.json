	"scripts": {
		"build": "gatsby build",
		"predeploy": "yarn build",
		"deploy": "gatsby-plugin-s3 deploy --yes",
		"develop": "gatsby develop",
		"eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
		"fix": "concurrently -p \"[fix - {name}]\" -n \"eslint,prettier\" -c \"yellow.bold,green.bold\" \"yarn eslint --fix .\" \"yarn prettier --write **/*.{yml,json,md}\"",
		"lint": "concurrently -p \"[lint - {name}]\" -n \"eslint,prettier\" -c \"yellow.bold,green.bold\" \"yarn eslint .\" \"yarn prettier --check **/*.{yml,json,md}\"",
		"prepare": "husky",
		"start": "yarn develop",
		"serve": "gatsby serve",
		"test": "concurrently -p \"[test - {name}]\" -n \"lint,type-check\" -c \"cyan.bold,blue.bold\" \"yarn lint\" \"yarn type-check\"",
		"type-check": "tsc --project tsconfig.json --pretty --noEmit"
	}
